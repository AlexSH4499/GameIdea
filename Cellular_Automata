#include<stdio.h>
#define COLS (3)
#define ROWS (3)

//Initialize the board and assign it a state
void initialize(int* board, int state)
{
  //Loop through 2D array to feed values
  for(int i =0; i < ROWS -1 ; i++)
  {
    for(int j = 0; j < COLS -1 ;j++ )
    {
      //Use randomized seeder
      //It must be either a 1 or a 0
      board[i][j] = random(2);
      
    }
  }
  
}

//Defines the rules for our CA
void behaviour(int* board, int index, int state)
{
  
}

//Creates our cell-system that will be presented
void createCell()
{
  int board[ROWS][COLS];//3 x 3 matrix to be utilized
  int state;
  
  int previous;
  int current;
  
}
